{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "lit-a11y/click-events-have-key-events": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "react/forbid-prop-types": "off",
        "camelcase": "off"
    }
}
